<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.sravyanaguboyina.eventbuzz.select">

    <Button
        android:id="@+id/staffsign"
        style="@style/Widget.AppCompat.Button.Colored"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="102dp"
        android:layout_marginStart="102dp"
        android:layout_marginTop="140dp"
        android:text="SIGN IN AS STAFF"
        tools:layout_editor_absoluteX="135dp"
        tools:layout_editor_absoluteY="97dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:id="@+id/stusign"
        style="@style/Widget.AppCompat.Button.Colored"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/staffsign"
        android:layout_alignStart="@+id/staffsign"
        android:layout_below="@+id/staffsign"
        android:layout_marginTop="89dp"
        android:text="SIGN IN AS STUDENT" />


</RelativeLayout>
